program uendel;
    var uendel, luiz, neto, couto : integer;
    var x: array[1 .. 5] of integer; 
begin
    if 2 > 1 then
        begin
            uendel[7*(4+5)] := luiz = neto;
            couto := x + x;
        end
    else
        begin
            x := x + 2 + 2 * 3 / 9;
            luiz :=  luiz * luiz;
            uendel[7*(4+5)] := luiz = neto;
        end
    ;
end.
